{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/app/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ],
  "functions": {
    "vercel_app.py": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "env": {
    "FLASK_ENV": "production",
    "FLASK_DEBUG": "False"
  }
}